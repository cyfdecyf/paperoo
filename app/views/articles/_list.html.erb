<table class="zebra-striped">
 <thead>
  <tr>
    <th>Conference</th>
    <th>Title</th>
    <th>Actions</th>
  </tr>
  </thead>
  <tbody>
  <% articles.each do |article| -%>
   <tr>
    <td><%= article.conference.title rescue '' %></td>
    <td><%= link_to article.title, article_path(article) %>
    <td>
      <%= link_to "Show", article_path(article) %>
      <%= link_to "Edit", edit_article_path(article) %>
      <%= link_to "Destroy", article_path(article), :method => :delete, :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}" %>
    </td>
    </tr>
    <% end -%>
   </tbody>
</table>
